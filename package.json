    "mocha": "^5.2.0",
    "stl-models": "^0.12.0"
  },
  "dependencies": {
    "buffer-converter": "^1.0.0",
    "text-encoding": "0.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lowfab/stl-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/Lowfab/stl-exporter/issues"
